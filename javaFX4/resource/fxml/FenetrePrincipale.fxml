<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>


<BorderPane
        xmlns="http://javafx.com/javafx/11.0.1"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="view.FenetrePrincipale">
    <left>
        <ListView fx:id="listeViewPromotion" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
    </left>

   <center>
      <FlowPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" >
          <ListView prefHeight="200.0" prefWidth="200.0" fx:id="listeViewPromotion2" />
      </FlowPane>
   </center>

   <right>
      <FlowPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </right>

   <bottom>
      <FlowPane BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="Ajouter Etudiant" onAction="#ajouterEtudiant"/>
         </children>
      </FlowPane>
   </bottom>
</BorderPane>
